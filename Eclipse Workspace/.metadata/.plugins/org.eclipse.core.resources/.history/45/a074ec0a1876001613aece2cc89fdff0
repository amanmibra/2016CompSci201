
  public class Gravity {
      public double falling(double time, double velo){
		return velo;
        double disp = (velo * time) + (0.5 * 9.81 * Math.pow(time, 2));
        return disp; 
      }
      public static void main (String args[]){
    	  Gravity test = new Gravity;
    	  System.out.println(test.falling(3, 5));
      }
  }