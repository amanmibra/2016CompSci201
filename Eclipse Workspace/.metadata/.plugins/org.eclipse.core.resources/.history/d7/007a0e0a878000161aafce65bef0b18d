import java.util.*;

public class Anonymous {
	public int howMany(String[] headlines, String[] messages) {
		int ans = 0;
		String collectH = "";
		for(String s : headlines){
			collectH = collectH + s; 
		}
		collectH = collectH.toLowerCase();
		char[] headChar = collectH.toCharArray();
		return ans;
	}
}
